<h2>再予約する</h2>

<%= flash[:notice] %>
<%= flash[:alert] %>

<%= form_with model: @reservation do |f| %>

<table>
<tr>
<th><%= f.label :start_date, "開始日" %></th>
<td><%= f.text_field :start_date%></td>

<th><%= f.label :end_date, "終了日" %></th>
<td><%= f.text_field :end_date%></td>

<th><%= f.label :people, "人数" %></th>
<td><%= f.text_field :people%></td>

</tr>
</table>

<div>
<ul>
<li><%= f.submit "この日程で予約する"%></li>
</ul>
</div>

<% end %>

<p><%= link_to"トップページへ", root_path%></p>